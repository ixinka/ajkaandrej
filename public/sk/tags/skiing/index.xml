        &lt;/a&gt;
        
        &lt;a style=&#34;display:inline-block;&#34; class=&#34;fancybox&#34; href=&#34;https://www.ajka-andrej.com/wp-content/gallery/switzerland/laax/IMG_0030.jpg&#34; rel=&#34;group1&#34;&gt;
            &lt;img src=&#34;https://www.ajka-andrej.com/wp-content/gallery/switzerland/laax/IMG_0030_thumb.jpg&#34; itemprop=&#34;image&#34; width=&#34;100&#34; height=&#34;75&#34;&gt;
        &lt;/a&gt;
        
        &lt;a style=&#34;display:inline-block;&#34; class=&#34;fancybox&#34; href=&#34;https://www.ajka-andrej.com/wp-content/gallery/switzerland/laax/IMG_0031.jpg&#34; rel=&#34;group1&#34;&gt;
            &lt;img src=&#34;https://www.ajka-andrej.com/wp-content/gallery/switzerland/laax/IMG_0031_thumb.jpg&#34; itemprop=&#34;image&#34; width=&#34;100&#34; height=&#34;75&#34;&gt;
